NODE_ENV=production
PORT=3000
JWT_SECRET=your-super-secure-jwt-key-here
JWT_EXPIRATION=24h
# You can use either DATABASE_URL or individual DB credentials
DATABASE_URL=postgresql://postgres:your-password@db.your-project.supabase.co:5432/postgres
# Individual database credentials (only needed if DATABASE_URL is not set)
DB_HOST=your-railway-db-host
DB_NAME=jantungin
DB_USER=postgres
DB_PASSWORD=your-secure-db-password
DB_PORT=5432
ENCRYPTION_KEY=your-32-character-encryption-key
ALLOWED_ORIGINS=https://your-frontend-url.com,http://localhost:5173
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
ADMIN_EMAIL=admin@jantungin.com
ADMIN_PASSWORD=your-secure-admin-password

# ENDPOINTS
ENDPOINTS_BASE_URL=https://jantungin-api.up.railway.app